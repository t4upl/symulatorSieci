	int countVolume(int i, int j, int[] vector, int max)
	{
		int sum=0;
		i++;
		while (i<j)
		{
			int d = max -vector[i];
			
			if (d<0)
			{
				System.out.println("Soemthing went wrong");
			}
			sum+=d;
			
			i++;
		}
		
		return sum;
