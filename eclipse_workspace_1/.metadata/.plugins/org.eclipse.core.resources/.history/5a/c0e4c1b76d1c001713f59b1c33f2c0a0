
public class EVData {

	//0 - dom, 1- podroz z domu do pracy, 2 - praca, 3 - podroz z pracy do domu
	int status;

	double EVdom;
	double EVdom_c; 
	double EVdom_EB;
	double EB_EVdom;
	double G_EVdom;
	double Zew_EVdom;
	
	
	double EVpraca;
	double EVpraca_c;
	double EVpraca_EB;
	double EB_EVpraca;
	double G_EVpraca;
	double Zew_EVpraca;
	
	public int getStatus() {
		return status;
	}

	public void setStatus(int status) {
		this.status = status;
	}


	
	
	
}
